.navbar {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding: 0 14px;
  box-sizing: border-box;
  z-index: 9999;
}

.content {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 34rpx;
}

.content>.img{
  width: 36rpx;
  height: 36rpx;
}