<view class="notice">
  <view class="img">
    <image src="/images/notice.png" mode="aspectFill"></image>
  </view>
  <swiper class="text" autoplay="true" interval="3000" duration="500" vertical="true">
    <swiper-item wx:for="{{list}}" wx:key="index">
      {{item}}
    </swiper-item>
  </swiper>
</view>